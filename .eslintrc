{
  "extends": "airbnb-base",
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "comma-dangle": ["error", "never"],
    "import/prefer-default-export": "off",
    "function-paren-newline": "off",
    "func-names": "off",
    "import/no-dynamic-require": "off",
    "no-param-reassign": "off"
  }
}
